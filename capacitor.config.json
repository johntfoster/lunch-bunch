{
  "appId": "com.johntfoster.lunchbunch",
  "appName": "Lunch Bunch",
  "webDir": "src",
  "backgroundColor": "#1a1a1a",
  "plugins": {
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": ["google.com"]
    }
  },
  "ios": {
    "scheme": "Lunch Bunch",
    "contentInset": "always"
  },
  "server": {
    "androidScheme": "https",
    "allowNavigation": [
      "*.googleapis.com",
      "*.firebaseapp.com",
      "*.web.app",
      "accounts.google.com"
    ]
  }
}
